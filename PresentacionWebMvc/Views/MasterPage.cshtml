@using Dominio
@using Servicio
<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="msapplication-tap-highlight" content="no">
    <meta name="description" content="Materialize is a Material Design Admin Template,It's modern, responsive and based on Material Design by Google. ">
    <meta name="keywords" content="materialize, admin template, dashboard template, flat admin template, responsive admin template,">
    <title>@ViewBag.Title</title>

    <!-- Favicons-->
    <link rel="icon" href="~/Design/images/favicon/favicon-32x32.png" sizes="32x32">
    <!-- Favicons-->
    <link rel="apple-touch-icon-precomposed" href="~/Design/images/favicon/apple-touch-icon-152x152.png">
    <!-- For iPhone -->
    <meta name="msapplication-TileColor" content="#00bcd4">
    <meta name="msapplication-TileImage" content="~/Design/images/favicon/mstile-144x144.png">
    <!-- For Windows Phone -->
    <!-- CORE CSS-->

    <link href="~/Design/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="~/Design/css/style.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="http://cdn.datatables.net/1.10.6/css/jquery.dataTables.min.css" type="text/css" rel="stylesheet" media="screen,projection">

    <!-- INCLUDED PLUGIN CSS ON THIS PAGE -->
    <link href="~/Design/css/prism.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="~/Design/js/plugins/perfect-scrollbar/perfect-scrollbar.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="~/Design/js/plugins/data-tables/css/jquery.dataTables.min.css" type="text/css" rel="stylesheet" media="screen,projection">
    <link href="~/Design/js/plugins/chartist-js/chartist.min.css" type="text/css" rel="stylesheet" media="screen,projection">
</head>

<body>
    <!-- Start Page Loading -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->
    
    <!-- START HEADER -->
    <header id="header" class="page-topbar">
        @{ entUsuario u = (entUsuario)Session["usuario"];
            String nomCompleto = u.Usuario;
        }
        <!-- start header nav-->
        <div class="navbar-fixed">
            <nav class="cyan">
                <div class="nav-wrapper">
                    <h1 class="logo-wrapper"><a href="@Url.Action("Principal","Almacen")" class="brand-logo darken-1"><img src="~/Design/images/materialize-logo.png" alt="materialize logo"></a> <span class="logo-text">Materialize</span></h1>
                    
                </div>
            </nav>
        </div>
        <!-- end header nav-->
    </header>
    <!-- END HEADER -->

    <!-- START MAIN -->
    <div id="main">
        <!-- START WRAPPER -->
        <div class="wrapper">
            
            <!-- START LEFT SIDEBAR NAV-->
            <aside id="left-sidebar-nav">
                <ul id="slide-out" class="side-nav fixed leftside-navigation">

                    <li style="text-align: center;">
                        Bienvenido @nomCompleto
                    </li>
                    <li class="divider"></li>
                    <li class="bold">
                        <a href="@Url.Action("MantenedorJefe", "Complejo")" class="waves-effect waves-cyan"><i class="mdi-action-dashboard"></i>Jefe Organizacion</a>
                    </li>
                    <li class="bold">
                        <a href="@Url.Action("MantenedorComplejo", "Complejo")" class="waves-effect waves-cyan"><i class="mdi-action-dashboard"></i>Complejo Deportivo</a>
                    </li>

                    <li>
                        <a href="@Url.Action("Index", "Inicio")"><i class="mdi-hardware-keyboard-tab"></i> Logout</a>
                    </li>

                </ul>
                <a href="#" data-activates="slide-out" class="sidebar-collapse btn-floating btn-medium waves-effect waves-light hide-on-large-only darken-2"><i class="mdi-navigation-menu"></i></a>
            </aside>
            <!-- END LEFT SIDEBAR NAV-->
            
            <!-- START CONTENT -->
            <section id="content">

                <!--breadcrumbs end-->
                <!--start container-->
                <div class="container">
                    <div class="section">

                        @RenderBody()
                        <div class="divider"></div>
                        <br>
                    </div>
                </div>
                <!--end container-->
            </section>
            <!-- END CONTENT -->
            
        </div>
        <!-- END WRAPPER -->

    </div>
    <!-- END MAIN -->
    
    <!-- START FOOTER -->
    <footer class="page-footer">
        <div class="footer-copyright">
            <div class="container">
                <span>Copyright © 2015 <a class="grey-text text-lighten-4" All rights reserved.</span>
                <span class="right"> Design and Developed by <a class="grey-text text-lighten-4" href="http://geekslabs.com/">GeeksLabs</a></span>
            </div>
        </div>
    </footer>
    <!-- END FOOTER -->
    <!-- jQuery Library -->
    <script type="text/javascript" src="~/Design/js/jquery-1.11.2.min.js"></script>
    <!--materialize js-->
    <script type="text/javascript" src="~/Design/js/materialize.min.js"></script>
    <!--prism-->
    <script type="text/javascript" src="~/Design/js/prism.js"></script>
    <!--scrollbar-->
    <script type="text/javascript" src="~/Design/js/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>

    <!-- chartist -->
    <script type="text/javascript" src="~/Design/js/plugins/chartist-js/chartist.min.js"></script>

    <script type="text/javascript" src="~/Design/js/plugins/data-tables/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Design/js/plugins/data-tables/data-tables-script.js"></script>

    <!-- sparkline -->
    <script type="text/javascript" src="~/Design/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="~/Design/js/plugins/sparkline/sparkline-script.js"></script>

    <!--jvectormap-->
    <script type="text/javascript" src="~/Design/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script type="text/javascript" src="~/Design/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script type="text/javascript" src="~/Design/js/plugins/jvectormap/vectormap-script.js"></script>

    <!--plugins.js - Some Specific JS codes for Plugin Settings-->
    <script type="text/javascript" src="~/Design/js/plugins.js"></script>

</body>

</html>
